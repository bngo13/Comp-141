    a
        expr ::= term + term
             ::= factor * factor + factor * factor
             ::= number * number + number * number
             ::= 78 * 20 + 5 * 39
    b
    c
2
    a
        expr ::= term [+ expr]
             ::= term + term
             ::= factor * factor * factor * factor
             ::= number * number + number * number
             ::= 78 * 20 + 5 * 39
    b
    c