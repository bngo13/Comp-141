Scanner:

Line: z := 0;
z : IDENTIFIER
:= : SYMBOL
0 : NUMBER
; : SYMBOL

Scanner:

Line: x := 10;
x : IDENTIFIER
:= : SYMBOL
10 : NUMBER
; : SYMBOL

Scanner:

Line: y := 0;
y : IDENTIFIER
:= : SYMBOL
0 : NUMBER
; : SYMBOL

Parser:

; : SYMBOL
	; : SYMBOL
		:= : SYMBOL
			z : IDENTIFIER
			0 : NUMBER
		:= : SYMBOL
			x : IDENTIFIER
			10 : NUMBER
	:= : SYMBOL
		y : IDENTIFIER
		0 : NUMBER


